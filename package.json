{
  "name": "simple-notes-demo",
  "version": "0.2.0",
  "description": "A simple note keeping application for demonstration purposes",
  "scripts": {
    "build": "rimraf public/assets && webpack --bail --progress --profile",
    "build-watch": "webpack --watch",
    "php-serve": "php -S localhost:8080 -t public",
    "server": "npm-run-all -p -r build-watch php-serve",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claylong/simple-notes-demo.git"
  },
  "author": "Clay Long",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/claylong/simple-notes-demo/issues"
  },
  "homepage": "https://github.com/claylong/simple-notes-demo",
  "dependencies": {
    "angular": "^1.6.4",
    "angular-deckgrid": "^0.5.0",
    "angular-loading-bar": "^0.9.0",
    "angular-ui-router": "^1.0.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router-uib-modal": "0.0.11",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "bootstrap": "^3.3.7",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^1.1.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "2.0.1",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "node-libs-browser": "2.0.0",
    "npm-run-all": "^4.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.4",
